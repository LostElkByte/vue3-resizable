{
  "name": "vue3-resizable",
  "version": "0.0.0-alpha.2",
  "description": "Vue3 is used for components with resizable and draggable elements.",
  "private": false,
  "type": "module",
  "main": "./dist/elk-resize.umd.cjs",
  "module": "./dist/elk-resize.js",
  "unpkg": "./dist/elk-resize.umd.cjs",
  "exports": {
    ".": {
      "types": "./dist/elk-resize.d.ts",
      "import": "./dist/elk-resize.js",
      "require": "./dist/elk-resize.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/elk-resize.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "author": {
    "name": "LostElk",
    "email": "lostelk.email@gmail.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "any-touch": "^2.2.0",
    "vue": "^3.4.21"
  },
  "keywords": [
    "vue",
    "vue3",
    "resize",
    "resizable",
    "component",
    "调整大小"
  ],
  "homepage": "https://github.com/LostElkByte/vue3-resizable",
  "bugs": "https://github.com/LostElkByte/vue3-resizable/issues",
  "license": "MIT",
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.2",
    "sass": "^1.72.0",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vue-tsc": "^2.0.6"
  }
}
